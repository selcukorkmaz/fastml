% Generated by hand for predict_risk and predict_survival
\name{predict_risk}
\alias{predict_risk}
\alias{predict_risk.fastml_native_survival}
\alias{predict_risk.workflow}
\alias{predict_survival}
\alias{predict_survival.fastml_native_survival}
\alias{predict_survival.workflow}
\title{Risk scores and survival curves for fastml models}
\description{
Compute linear predictors / risk scores or survival probability curves from
fitted survival models created within \pkg{fastml}. The helpers work with both
native engines (such as the xgboost survival integration) and parsnip
workflows.
}
\usage{
predict_risk(fit, newdata, ...)

\method{predict_risk}{fastml_native_survival}(fit, newdata, ...)

\method{predict_risk}{workflow}(fit, newdata, ...)

predict_survival(fit, newdata, times, ...)

\method{predict_survival}{fastml_native_survival}(fit, newdata, times, ...)

\method{predict_survival}{workflow}(fit, newdata, times, ...)
}
\arguments{
  \item{fit}{A fitted survival model, either a native \code{fastml} survival
    object or a parsnip workflow.}
  \item{newdata}{A data frame of predictor values. Outcome columns should be
    omitted.}
  \item{times}{Numeric vector of evaluation times at which survival
    probabilities are required.}
  \item{...}{Additional arguments passed to methods.}
}
\value{
\code{predict_risk()} returns a numeric vector of risk scores. Higher values
indicate greater risk. \code{predict_survival()} returns a numeric matrix with
one row per observation and one column per requested time.
}
\examples{
\dontrun{
library(survival)
library(fastml)

set.seed(42)
mod <- fastml(
  data = lung,
  label = c("time", "status"),
  algorithms = "xgboost",
  task = "survival",
  resampling_method = "none",
  test_size = 0.3,
  algorithm_engines = list(xgboost = "cox")
)

spec <- mod$models[[1]]
new_obs <- lung[1:3, setdiff(names(lung), c("time", "status"))]

risk <- predict_risk(spec, new_obs)
curve <- predict_survival(spec, new_obs, times = c(100, 200, 300))
}
}
